generator client {
  provider = "prisma-client"
  engineType = "client"
  output = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Admission {
  id            String   @id
  patientId     String
  status        String
  admittedAt    DateTime
  dischargedAt  DateTime?

  @@map("admissions")
}
